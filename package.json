{
  "name": "moinsen-flutter",
  "displayName": "Moinsen Flutter",
  "description": "Moinsen helps You with your Flutter stuff :-)",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:moinsenFlutter.manageIndex"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moinsenFlutter.manageIndex",
        "title": "Moinsen Flutter",
        "category": "Moinsen Flutter"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "moinsenFlutter.manageIndex",
          "when": "resourceScheme == 'file'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
