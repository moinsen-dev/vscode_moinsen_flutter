{
  "name": "moinsen-flutter",
  "displayName": "Moinsen Flutter",
  "description": "Moinsen helps You with your Flutter stuff :-)",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moinsenFlutter.manageIndex",
        "title": "Moinsen Flutter",
        "category": "Moinsen Flutter"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "moinsenFlutter.manageIndex",
          "when": "resourceScheme == 'file'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "mocha": "mocha --require ts-node/register src/test/**/*.test.ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "glob": "^10.3.10",
    "mocha": "^10.7.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
